<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="webkit" library="libwebkitgtk-1.0.so" depends="gtk+">
  <glade-widget-classes>
    <glade-widget-class name="WebkitWebView" generic-name="webkitwebview" title="WebKit Web View">
      <properties>
        <property id="view-mode" common="False" default="Windowed">
          <displayable-values>
            <value id="WEBKIT_WEB_VIEW_VIEW_MODE_WINDOWED" name="Windowed"/>
            <value id="WEBKIT_WEB_VIEW_VIEW_MODE_FLOATING" name="Floating"/>
            <value id="WEBKIT_WEB_VIEW_VIEW_MODE_FULLSCREEN" name="Fullscreen"/>
            <value id="WEBKIT_WEB_VIEW_VIEW_MODE_MAXIMIZED" name="Maximized"/>
            <value id="WEBKIT_WEB_VIEW_VIEW_MODE_MINIMIZED" name="Minimized"/>
          </displayable-values>
        </property>
      </properties>
    </glade-widget-class>
    <glade-widget-class name="WebkitWebFrame" generic-name="webkitwebframe" title="WebKit Web Frame"/>
    <glade-widget-class name="WebkitWebSettings" generic-name="webkitwebsettings" title="WebKit Web Settings">
      <properties>
        <property id="editing-behavior" common="False" default="Mac">
          <displayable-values>
            <value id="WEBKIT_EDITING_BEHAVIOR_MAC" name="Mac"/>
            <value id="WEBKIT_EDITING_BEHAVIOR_WINDOWS" name="Windows"/>
          </displayable-values>
        </property>
      </properties>
    </glade-widget-class>
    <glade-widget-class name="WebkitWebBackForwardList" generic-name="webkitwebbackforwardlist" title="WebKit Web Back Forward List"/>
    <glade-widget-class name="WebkitWebHistoryItem" generic-name="webkitwebhistoryitem" title="WebKit Web History Item" />
    <glade-widget-class name="WebkitWebNavigationAction" generic-name="webkitwebnavigationaction" title="WebKit Web Navigation Action">
      <properties>
        <property id="reason" common="False" default="Other">
          <displayable-values>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_LINK_CLICKED" name="Link clicked"/>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_FORM_SUBMITTED" name="Form submitted"/>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_BACK_FORWARD" name="Back/forward"/>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_RELOAD" name="Reload"/>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_FORM_RESUBMITTED" name="Form resubmitted"/>
            <value id="WEBKIT_WEB_NAVIGATION_REASON_OTHER" name="Other"/>
          </displayable-values>
        </property>
      </properties>
    </glade-widget-class>
    <glade-widget-class name="WebkitWebPolicyDecision" generic-name="webkitwebpolicydecision" title="WebKit Web Policy Decision"/>
    <glade-widget-class name="WebkitWebWindowFeatures" generic-name="webkitwebwindowfeatures" title="WebKit Web Window Features"/>
  </glade-widget-classes>
  <glade-widget-group name="webkit" title="WebKit">
    <glade-widget-class-ref name="WebkitWebView"/>
    <glade-widget-class-ref name="WebkitWebFrame"/>
    <glade-widget-class-ref name="WebkitWebSettings"/>
    <glade-widget-class-ref name="WebkitWebBackForwardList"/>
    <glade-widget-class-ref name="WebkitWebHistoryItem"/>
    <glade-widget-class-ref name="WebkitWebNavigationAction"/>
    <glade-widget-class-ref name="WebkitWebPolicyDecision"/>
    <glade-widget-class-ref name="WebkitWebWindowFeatures"/>
  </glade-widget-group>
</glade-catalog>
