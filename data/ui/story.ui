<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAdjustment" id="trim_adj">
    <property name="lower">1</property>
    <property name="upper">30</property>
    <property name="value">10</property>
    <property name="step_increment">1</property>
    <property name="page_increment">5</property>
  </object>
  <object class="GtkDialog" id="skein_trim_dialog">
    <property name="border_width">12</property>
    <property name="title" translatable="yes">Trim the skein</property>
    <property name="icon_name">inform7</property>
    <property name="type_hint">dialog</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox2">
        <property name="visible">True</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkLabel" id="label4">
            <property name="visible">True</property>
            <property name="label" translatable="yes">This will remove nodes from the skein and transcript that have not been visited recently, and which have not been locked. Drag the slider to the right to increase the number of nodes that will be deleted.

This operation cannot be undone.</property>
            <property name="wrap">True</property>
          </object>
          <packing>
            <property name="padding">6</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkHBox" id="hbox1">
            <property name="visible">True</property>
            <child>
              <object class="GtkLabel" id="label5">
                <property name="visible">True</property>
                <property name="xalign">0</property>
                <property name="label" translatable="yes">_Trim a little</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label6">
                <property name="visible">True</property>
                <property name="xalign">1</property>
                <property name="label" translatable="yes">Trim a lot</property>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkHScale" id="skein_trim_slider">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="adjustment">trim_adj</property>
            <property name="fill_level">100</property>
            <property name="draw_value">False</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">3</property>
          </packing>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area2">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
            <child>
              <object class="GtkButton" id="button5">
                <property name="label">gtk-cancel</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="use_stock">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="button4">
                <property name="label">gtk-ok</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="use_stock">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-6">button5</action-widget>
      <action-widget response="-5">button4</action-widget>
    </action-widgets>
  </object>
  <object class="GtkAdjustment" id="vspacing_adj">
    <property name="lower">20</property>
    <property name="upper">100</property>
    <property name="value">75</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="hspacing_adj">
    <property name="lower">20</property>
    <property name="upper">100</property>
    <property name="value">40</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkImage" id="image1">
    <property name="visible">True</property>
    <property name="stock">gtk-revert-to-saved</property>
  </object>
  <object class="GtkDialog" id="skein_spacing_dialog">
    <property name="border_width">12</property>
    <property name="title" translatable="yes">Skein spacing options</property>
    <property name="icon_name">inform7</property>
    <property name="type_hint">dialog</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkLabel" id="label3">
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">&lt;b&gt;Edit the spacing for items in the skein&lt;/b&gt;</property>
            <property name="use_markup">True</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkTable" id="table1">
            <property name="visible">True</property>
            <property name="border_width">6</property>
            <property name="n_rows">2</property>
            <property name="n_columns">2</property>
            <property name="column_spacing">12</property>
            <property name="row_spacing">6</property>
            <child>
              <object class="GtkHScale" id="skein_spacing_horizontal">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="adjustment">hspacing_adj</property>
                <property name="fill_level">100</property>
                <property name="draw_value">False</property>
                <signal name="value-changed" handler="on_skein_spacing_horizontal_value_changed" swapped="no"/>
              </object>
              <packing>
                <property name="top_attach">1</property>
                <property name="bottom_attach">2</property>
                <property name="y_options"></property>
              </packing>
            </child>
            <child>
              <object class="GtkHScale" id="skein_spacing_vertical">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="adjustment">vspacing_adj</property>
                <property name="draw_value">False</property>
                <signal name="value-changed" handler="on_skein_spacing_vertical_value_changed" swapped="no"/>
              </object>
              <packing>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">1</property>
                <property name="bottom_attach">2</property>
                <property name="y_options"></property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="visible">True</property>
                <property name="xalign">0</property>
                <property name="label" translatable="yes">_Horizontal spacing</property>
                <property name="use_underline">True</property>
                <property name="mnemonic_widget">skein_spacing_horizontal</property>
              </object>
              <packing>
                <property name="y_options"></property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label2">
                <property name="visible">True</property>
                <property name="xalign">0</property>
                <property name="label" translatable="yes">_Vertical spacing</property>
                <property name="use_underline">True</property>
                <property name="mnemonic_widget">skein_spacing_vertical</property>
              </object>
              <packing>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="y_options"></property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area1">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
            <child>
              <object class="GtkButton" id="skein_spacing_use_defaults">
                <property name="label" translatable="yes">_Use defaults</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="image">image1</property>
                <property name="use_underline">True</property>
                <signal name="clicked" handler="on_skein_spacing_use_defaults_clicked" swapped="no"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="button2">
                <property name="label">gtk-cancel</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="use_stock">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="button1">
                <property name="label">gtk-ok</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="use_stock">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">2</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="1">skein_spacing_use_defaults</action-widget>
      <action-widget response="-6">button2</action-widget>
      <action-widget response="-5">button1</action-widget>
    </action-widgets>
  </object>
  <object class="GtkToggleAction" id="view_notepad">
    <property name="label">N_otepad</property>
    <property name="short_label">View Notepad</property>
    <property name="tooltip">Show or hide the notepad</property>
    <signal name="toggled" handler="action_view_notepad_toggled" swapped="no"/>
  </object>
  <object class="GtkWindow" id="notes_window">
    <property name="title" translatable="yes">Notepad</property>
    <property name="default_width">300</property>
    <property name="default_height">300</property>
    <property name="icon_name">inform7</property>
    <property name="type_hint">utility</property>
    <property name="skip_taskbar_hint">True</property>
    <property name="skip_pager_hint">True</property>
    <signal name="delete-event" handler="on_notes_window_delete_event" swapped="no"/>
    <child>
      <object class="GtkScrolledWindow" id="scrolledwindow1">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="hscrollbar_policy">automatic</property>
        <property name="vscrollbar_policy">automatic</property>
        <child>
          <object class="GtkTextView" id="notes_view">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="wrap_mode">word-char</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkAction" id="export_ifiction_record">
    <property name="label">_Export iFiction Record...</property>
    <property name="tooltip">Save this story's iFiction metadata</property>
    <signal name="activate" handler="action_export_ifiction_record" swapped="no"/>
  </object>
  <object class="GtkAction" id="open_materials_folder">
    <property name="label">_Open Materials Folder</property>
    <property name="tooltip">Open this story's Materials folder in the file manager</property>
    <property name="stock_id">gtk-directory</property>
    <signal name="activate" handler="action_open_materials_folder" swapped="no"/>
  </object>
  <object class="GtkAction" id="test_me">
    <property name="label">_Test Me</property>
    <property name="tooltip">Compile the story, and run the "test me" command</property>
    <signal name="activate" handler="action_test_me" swapped="no"/>
  </object>
  <object class="GtkAction" id="release_menu">
    <property name="label">Re_lease</property>
  </object>
  <object class="GtkAction" id="replay_menu">
    <property name="label">_Replay</property>
  </object>
  <object class="GtkAction" id="play_menu">
    <property name="label">_Play</property>
  </object>
  <object class="GtkHPaned" id="facing_pages">
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <property name="position">1</property>
    <property name="position_set">True</property>
    <signal name="set-focus-child" handler="on_facing_pages_set_focus_child" swapped="no"/>
    <child>
      <placeholder/>
    </child>
  </object>
  <object class="GtkAction" id="save_debug_build">
    <property name="label">Release for _Testing...</property>
    <property name="tooltip">Compile the story with debugging features on and export it</property>
    <signal name="activate" handler="action_save_debug_build" swapped="no"/>
  </object>
  <object class="GtkAction" id="release">
    <property name="label">_Release...</property>
    <property name="short_label">_Release</property>
    <property name="tooltip">Compile the story ready for release</property>
    <property name="stock_id">gtk-cdrom</property>
    <signal name="activate" handler="action_release" swapped="no"/>
  </object>
  <object class="GtkAction" id="play_all_blessed">
    <property name="label">Re_play Commands Blessed in Transcript</property>
    <property name="tooltip">Play through all the blessed nodes in the entire Skein</property>
    <signal name="activate" handler="action_play_all_blessed" swapped="no"/>
  </object>
  <object class="GtkAction" id="stop">
    <property name="label">_Stop</property>
    <property name="tooltip">Stop the story that's currently running</property>
    <property name="stock_id">gtk-media-stop</property>
    <signal name="activate" handler="action_stop" swapped="no"/>
  </object>
  <object class="GtkAction" id="replay">
    <property name="label">_Replay Last Commands</property>
    <property name="short_label">Replay</property>
    <property name="tooltip">Compile the story, and re-run to the previously active play point</property>
    <property name="stock_id">gtk-media-rewind</property>
    <signal name="activate" handler="action_replay" swapped="no"/>
  </object>
  <object class="GtkAction" id="go">
    <property name="label">_Go</property>
    <property name="short_label">_Go!</property>
    <property name="tooltip">Compile and run the story</property>
    <property name="stock_id">gtk-media-play</property>
    <signal name="activate" handler="action_go" swapped="no"/>
  </object>
  <object class="GtkAction" id="refresh_index">
    <property name="label">Refresh _Index</property>
    <property name="tooltip">Recompile the index files and display them</property>
    <property name="stock_id">gtk-refresh</property>
    <signal name="activate" handler="action_refresh_index" swapped="no"/>
  </object>
  <object class="GtkAction" id="next_difference_skein">
    <property name="label">Show Next _Difference in Skein</property>
    <property name="tooltip">Go to the next difference in the Skein</property>
    <signal name="activate" handler="action_next_difference_skein" swapped="no"/>
  </object>
