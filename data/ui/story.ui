    <property name="tooltip">Show the last command in the Skein</property>
    <signal name="activate" handler="action_show_last_command_skein" swapped="no"/>
  </object>
  <object class="GtkAction" id="show_last_command">
    <property name="label">Show _Last Command</property>
    <property name="tooltip">Show the last command</property>
    <signal name="activate" handler="action_show_last_command" swapped="no"/>
  </object>
  <object class="GtkAction" id="import_into_skein">
    <property name="label">_Import into Skein...</property>
    <property name="tooltip">Import a play transcript into this story's Skein</property>
  </object>
  <object class="GtkActionGroup" id="unimplemented_actions">
    <property name="sensitive">False</property>
  </object>
  <object class="GtkAction" id="help_recipe_book">
